{
  "_version": "1.14.0",
  "sap.app": {
    "applicationVersion": {"version": "0.0.0"},
    "description": "{{appDescription}}",
    "i18n": "i18n/i18n.properties",
    "id": "sap.ui.demo.basicTemplate",
    "title": "{{appTitle}}",
    "type": "application"
  },
  "sap.ui": {
    "deviceTypes": {
      "desktop": true,
      "phone": true,
      "tablet": true
    },
    "technology": "UI5"
  },
  "sap.ui5": {
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "dependencies": {
      "minUI5Version": "1.62.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {}
      }
    },
    "handleValidation": true,
    "models": {
      "i18n": {
        "settings": {
          "bundleName": "sap.ui.demo.basicTemplate.i18n.i18n"
        },
        "type": "sap.ui.model.resource.ResourceModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "rootView": {
      "async": true,
      "viewName": "sap.ui.demo.basicTemplate.view.App",
      "type": "XML"
    },
    "routing": {
      "config": {
        "async": true,
        "controlAggregation": "pages",
        "controlId": "app",
        "routerClass": "sap.m.routing.Router",
        "viewPath": "sap.ui.demo.basicTemplate.view",
        "viewType": "XML"
      },
      "routes": [
        {
          "name": "home",
          "pattern": "",
          "target": ["home"]
        }
      ],
      "targets": {
        "home": {
          "viewId": "home",
          "viewLevel": 1,
          "viewName": "Home",
          "title": "{i18n>title}"
        }
      }
    }
  }
}
